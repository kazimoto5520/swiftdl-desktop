{
  "manifest_version": 3,
  "name": "SwiftDL Downloader",
  "version": "1.0",
  "description": "Download using SwiftDL desktop app",
  "permissions": ["downloads", "contextMenus", "nativeMessaging"],
  "action": {
    "default_popup": "popup.html"
  },
  "background": {
    "service_worker": "background.js"
  }
}
